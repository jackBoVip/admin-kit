{
  "name": "@admin-core/ui",
  "version": "0.0.8",
  "private": false,
  "type": "module",
  "description": "基础 UI 组件库",
  "keywords": [
    "admin",
    "admin-kit",
    "admin-core",
    "admin-panel",
    "ui",
    "ui-components",
    "ui-library",
    "ui-framework",
    "vue",
    "vue3",
    "vue-components",
    "components",
    "component-library",
    "library",
    "frontend",
    "frontend-development",
    "web-components",
    "design-system",
    "interactive",
    "widgets",
    "elements",
    "typescript",
    "javascript",
    "modular",
    "customizable",
    "reusable"
  ],
  "author": "Admin Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackBoVip/admin-kit.git",
    "directory": "packages/ui"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "prepublishOnly": "node ../../scripts/resolve-catalog.js",
    "postpublish": "node ../../scripts/restore-catalog.js"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "@vueuse/core": "^14.0.0",
    "reka-ui": "^2.0.0",
    "lucide-vue-next": "^0.500.0"
  },
  "dependencies": {
    "@admin-core/composables": "workspace:*",
    "@admin-core/icons": "workspace:*",
    "@admin-core/shared": "workspace:*",
    "class-variance-authority": "catalog:",
    "vee-validate": "catalog:"
  }
}
