{
  "extends": "@admin-kit/tsconfig/vue.json",
  "compilerOptions": {
    "outDir": "./dist",
    "rootDir": "./src",
    "composite": false,
    "noEmit": false,
    "allowImportingTsExtensions": false,
    "exactOptionalPropertyTypes": false,
    "lib": ["ESNext", "DOM", "DOM.Iterable"],
    "target": "ESNext",
    "module": "ESNext"
  },
  "include": ["src/**/*"]
}
