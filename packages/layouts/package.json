{
  "name": "@admin-core/layouts",
  "version": "0.0.8",
  "private": false,
  "type": "module",
  "description": "布局和业务组件库",
  "keywords": [
    "admin",
    "admin-kit",
    "admin-core",
    "layouts",
    "layout",
    "business-components",
    "vue",
    "vue3",
    "component-library"
  ],
  "author": "Admin Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackBoVip/admin-kit.git",
    "directory": "packages/layouts"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./dist/style.css": "./dist/style.css"
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "types": "./dist/index.d.ts",
  "unpkg": "./dist/index.umd.js",
  "jsdelivr": "./dist/index.umd.js",
  "files": [
    "dist",
    "README.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "vite build && vue-tsc --declaration --emitDeclarationOnly --outDir dist",
    "dev": "vite build --watch",
    "prepublishOnly": "node ../../scripts/resolve-catalog.js",
    "postpublish": "node ../../scripts/restore-catalog.js"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "vue-router": "^4.0.0",
    "@vueuse/core": "^14.0.0"
  },
  "dependencies": {
    "@admin-core/ui": "workspace:*",
    "@admin-core/composables": "workspace:*",
    "@admin-core/icons": "workspace:*",
    "@admin-core/shared": "workspace:*",
    "@vee-validate/zod": "catalog:",
    "vee-validate": "catalog:",
    "zod": "catalog:",
    "zod-defaults": "catalog:"
  }
}

