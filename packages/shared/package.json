{
  "name": "@admin-core/shared",
  "version": "0.4.2",
  "private": false,
  "type": "module",
  "description": "共享工具、常量和类型",
  "keywords": [
    "admin",
    "admin-kit",
    "admin-core",
    "admin-panel",
    "admin-dashboard",
    "backend-admin",
    "management-system",
    "shared",
    "utils",
    "utilities",
    "helpers",
    "constants",
    "types",
    "typescript",
    "javascript",
    "vue",
    "vue3",
    "react",
    "frontend",
    "ui-library",
    "component-library",
    "color",
    "color-utils",
    "color-generator",
    "theme",
    "theming",
    "configuration",
    "validation",
    "common-functions",
    "tree-shaking",
    "esm",
    "cjs",
    "subpath-imports",
    "modular",
    "lightweight",
    "monorepo"
  ],
  "author": "Admin Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackBoVip/admin-kit.git",
    "directory": "packages/shared"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    },
    "./cache": {
      "types": "./dist/cache.d.ts",
      "import": "./dist/cache.js",
      "require": "./dist/cache.cjs"
    },
    "./color": {
      "types": "./dist/color.d.ts",
      "import": "./dist/color.js",
      "require": "./dist/color.cjs"
    },
    "./constants": {
      "types": "./dist/constants.d.ts",
      "import": "./dist/constants.js",
      "require": "./dist/constants.cjs"
    },
    "./types": {
      "types": "./dist/types.d.ts",
      "import": "./dist/types.js",
      "require": "./dist/types.cjs"
    },
    "./utils": {
      "types": "./dist/utils.d.ts",
      "import": "./dist/utils.js",
      "require": "./dist/utils.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.standalone.min.js",
  "jsdelivr": "./dist/index.umd.standalone.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README.en.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "node ../../scripts/resolve-catalog.js",
    "postpublish": "node ../../scripts/restore-catalog.js"
  },
  "dependencies": {
    "@ctrl/tinycolor": "catalog:",
    "@vue/shared": "catalog:",
    "@tanstack/vue-store": "catalog:",
    "clsx": "catalog:",
    "dayjs": "catalog:",
    "defu": "catalog:",
    "es-toolkit": "catalog:",
    "lodash.clonedeep": "catalog:",
    "nprogress": "catalog:",
    "tailwind-merge": "catalog:",
    "theme-colors": "catalog:"
  },
  "devDependencies": {
    "@types/lodash.clonedeep": "catalog:",
    "@types/nprogress": "catalog:"
  }
}
