{
  "name": "@admin-core/icons",
  "version": "0.1.4",
  "private": false,
  "type": "module",
  "description": "图标组件库",
  "keywords": [
    "admin",
    "admin-kit",
    "admin-core",
    "icons",
    "icon-library",
    "icon-components",
    "svg-icons",
    "svg",
    "vue",
    "vue3",
    "vue-icons",
    "components",
    "frontend",
    "ui",
    "interface",
    "graphics",
    "vector",
    "lucide",
    "lucide-icons",
    "iconify",
    "icon-pack",
    "symbols",
    "pictograms",
    "glyphs"
  ],
  "author": "Admin Kit Team",
  "license": "MIT",
  "repository": {
    "type": "git",
    "url": "https://github.com/jackBoVip/admin-kit.git",
    "directory": "packages/icons"
  },
  "exports": {
    ".": {
      "types": "./dist/index.d.ts",
      "import": "./dist/index.js",
      "require": "./dist/index.cjs"
    }
  },
  "main": "./dist/index.cjs",
  "module": "./dist/index.js",
  "unpkg": "./dist/index.umd.min.js",
  "jsdelivr": "./dist/index.umd.min.js",
  "types": "./dist/index.d.ts",
  "files": [
    "dist",
    "README.md",
    "README.en.md"
  ],
  "publishConfig": {
    "access": "public"
  },
  "scripts": {
    "build": "tsup",
    "dev": "tsup --watch",
    "prepublishOnly": "node ../../scripts/resolve-catalog.js",
    "postpublish": "node ../../scripts/restore-catalog.js"
  },
  "dependencies": {
    "@iconify/vue": "catalog:"
  },
  "peerDependencies": {
    "vue": "^3.5.0",
    "lucide-vue-next": "^0.500.0"
